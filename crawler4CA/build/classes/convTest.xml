    <script type="text/javascript"><![CDATA[
        $(document).ready(function () {

            //Sign In State Box
            $("#signstate-account-dropdown > .account").click(function () {
                $("#signstate-account-dropdown > .dropdown").fadeToggle("fast", function () {
                    if ($(this).css('display') == "none")
                        $("#signstate-account-dropdown > .account").removeClass("active");
                    else
                        $("#signstate-account-dropdown > .account").addClass("active");
                });
            });


            //iProperty Network
            $('li#ippnetworks').mouseover(function () { //When trigger is clicked...

                //Following events are applied to the subnav itself (moving subnav up and down)
                $(this).parent().find("ul.ippnetworks-ui").fadeIn('fast').show(); //Drop down the subnav on click

                $(this).parent().hover(function () {
                }, function () {
                    $(this).parent().find("ul.ippnetworks-ui").fadeOut('fast'); //When the mouse hovers out of the subnav, move it back up
                });
            });

            //back-to-top scrolling
            $('.back-to-top').css('display', 'none');

            $('a.back-to-top').click(function (e) {
                $('html, body').animate({ scrollTop: 0 }, 'slow');
                e.preventDefault();
            });

            $(window).scroll(function () {
                if ($('body').offset().top < $(window).scrollTop()) {
                    $('.back-to-top').slideDown('fast');
                } else {
                    $('.back-to-top').slideUp('fast');
                }
            });
        });
    ]]></script>

    
        

    
